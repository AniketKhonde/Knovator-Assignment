{
  "name": "knovator-job-importer-server",
  "version": "1.0.0",
  "description": "Backend server for Knovator Job Importer with queue processing and MongoDB storage",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:with-check": "node startup.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "import:manual": "node src/scripts/triggerImport.js",
    "keep-alive": "node keep-alive.js"

  },
  "keywords": [
    "job-importer",
    "queue",
    "mongodb",
    "redis",
    "bullmq",
    "xml-parser"
  ],
  "author": "Knovator Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "bullmq": "^4.12.0",
    "redis": "^4.6.10",
    "axios": "^1.6.2",
    "xml2js": "^0.6.2",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
